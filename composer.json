{
    "name": "hcantave/plaid-sdk-laravel",
    "description": "Laravel implementation of Plaid's API as an SDK.",
    "license": "MIT",
    "authors": [
        {
            "name": "Kyle Klaus",
            "email": "kklaus@indemnity83.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/http": "^9.0|^10.0|^11.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "moneyphp/money": "^4.7",
        "spatie/data-transfer-object": "^3.7",
        "spatie/laravel-data": "^3.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "CaashApp\\Plaid\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CaashApp\\Plaid\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CaashApp\\Plaid\\PlaidServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "rector/rector": "*"
    }
}
